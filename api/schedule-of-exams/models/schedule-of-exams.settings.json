{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "schedule_of_exams",
  "info": {
    "name": "Графік академічних концертів, заліків та іспитів"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "date": {
      "type": "datetime",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "year": {
      "type": "integer",
      "required": true,
      "default": 6
    }
  }
}
